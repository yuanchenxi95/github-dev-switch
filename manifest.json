{
  "name": "Github Dev Switch",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "72": "icons/72.png",
      "128": "icons/128.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "72": "icons/72.png",
    "128": "icons/128.png"
  },
  "permissions": ["tabs", "declarativeContent"]
}
